@article{MANASIJEVIC2021106685,
title = {Structural and thermal properties of Sn–Ag alloys},
journal = {Solid State Sciences},
volume = {119},
pages = {106685},
year = {2021},
issn = {1293-2558},
doi = {https://doi.org/10.1016/j.solidstatesciences.2021.106685},
